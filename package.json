{
  "name": "simple-amqplib-rpc",
  "description": "simple amqp rpc interface",
  "version": "1.0.2",
  "author": "Andrew Harris <andrew@noblesamurai.com>",
  "license": "BSD",
  "main": "src/index.js",
  "files": [
    "/src"
  ],
  "scripts": {
    "coverage": "c8 npm test",
    "docs": "jsdoc2md src/*.js",
    "pretest": "semistandard",
    "test": "mocha --recursive test",
    "watch": "mocha --recursive --watch test"
  },
  "homepage": "https://github.com/noblesamurai/node-simple-amqplib-rpc",
  "repository": {
    "type": "git",
    "url": "git://github.com/noblesamurai/node-simple-amqplib-rpc.git"
  },
  "bugs": {
    "url": "https://github.com/noblesamurai/node-simple-amqplib-rpc/issues"
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": "6.x"
  },
  "dependencies": {
    "amqplib": "^0.6.0",
    "error-ex": "^1.3.2",
    "p-event": "^3.0.0"
  },
  "devDependencies": {
    "c8": "^7.7.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "dirty-chai": "^2.0.1",
    "jsdoc-to-markdown": "^7.0.1",
    "mocha": "^8.3.2",
    "semistandard": "^16.0.0",
    "sinon": "^10.0.0",
    "sinon-chai": "^3.3.0"
  },
  "keywords": [
    "amqp",
    "amqplib",
    "rabbit",
    "rabbitmq",
    "rpc"
  ],
  "c8": {
    "include": [
      "src"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "all": true
  },
  "semistandard": {
    "env": [
      "mocha"
    ]
  }
}
